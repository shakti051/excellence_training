void main() {
  var euCountries = {'Italy', "U.k", "Russia"};
  var asianCountries = {'India', 'China', 'Russia'};
  print(euCountries.union(asianCountries));
  print(euCountries.intersection(asianCountries));
  print(euCountries.difference(asianCountries));
  const a = {1,3};
  const b = {3,5};
  int sum = 0;
  final c = a.difference(b).union(b.difference(a));
  print(c);//output {1,5}
  //sum of set c
  for(var value in c){
    sum +=value;
  }
  print("The sum is $sum");
}
